{
  "name": "@wormslab/vuetify-query-builder",
  "version": "1.0.10",
  "private": false,
  "scripts": {
    "serve": "NODE_ENV=development vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build --target lib --name vuetify-query-builder src/QueryBuilder.vue",
    "lint": "vue-cli-service lint"
  },
  "main": "./dist/vuetify-query-builder.common.js",
  "dependencies": {
    "dot-object": "^1.7.0",
    "moment": "^2.22.2",
    "underscore": "^1.9.1",
    "vee-validate": "^2.1.0-beta.7",
    "vue": "^2.5.16",
    "vuedraggable": "^2.16.0",
    "vuetify": "^1.1.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.5",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.5",
    "@vue/cli-service": "^3.0.0-rc.5",
    "css-loader": "^1.0.0",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
